#!/bin/bash
cd ./AFNetworking
xcodebuild -scheme "AFNetworking iOS" ONLY_ACTIVE_ARCH=NO -configuration Debug
xcodebuild -scheme "AFNetworking iOS" ONLY_ACTIVE_ARCH=NO -configuration Release
xcodebuild -scheme "AFNetworking iOS" ONLY_ACTIVE_ARCH=NO -configuration Debug -destination 'platform=iOS Simulator,name=iPhone 6s'
xcodebuild -scheme "AFNetworking iOS" ONLY_ACTIVE_ARCH=NO -configuration Release -destination 'platform=iOS Simulator,name=iPhone 6s'
cd ..
